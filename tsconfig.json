{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "dist",
    "baseUrl": "./src",
    "sourceMap": true,
    "removeComments": false,
    "module": "commonjs",
    "target": "es2017",
    "declaration": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": [
      "node"
    ],
    "lib": [
      "es7"
    ],
    "moduleResolution": "node",
    "paths": {
      "@cell/*": ["*"],
      "@share/*": ["$share/*"]
    }
  }
}